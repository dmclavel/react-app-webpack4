{
    "presets": [
        [
            "@babel/env",
            {
                "targets": {
                    "browsers": [
                        "> 1%",
                        "last 2 versions"
                    ]
                },
                "useBuiltIns": "entry",
            }
        ],
        "@babel/react"
    ],
    "plugins": [
        "transform-es2015-modules-commonjs",
        "@babel/plugin-syntax-dynamic-import",
        "@babel/plugin-proposal-class-properties"
    ]
}
